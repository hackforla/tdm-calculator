{
  "name": "tdm-calculator-client",
  "version": "0.2.29",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.json --ignore-path .eslintignore  \"**/*.{js,jsx}\"",
    "lint:fix": "eslint -c .eslintrc.json --ignore-path .eslintignore --fix  \"**/*.{js,jsx}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@microsoft/applicationinsights-react-js": "^3.0.5",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "formik": "^2.2.6",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-gtm-module": "^2.0.11",
    "react-input-mask": "^2.0.4",
    "react-jss": "^10.5.1",
    "react-loader": "^2.4.7",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-string-replace": "^0.4.4",
    "react-tooltip": "^4.2.13",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.7",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "require-context.macro": "^1.2.2",
    "sass": "^1.32.6"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write",
    "*.js": [
      "lint",
      "test"
    ]
  }
