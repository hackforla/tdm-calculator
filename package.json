{
  "name": "root",
  "version": "0.2.59",
  "private": true,
  "scripts": {
    "release-notes": "gren release --override",
    "start": "concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run start:backend\" \"npm run start:frontend\" ",
    "start:backend": "cd ./server && npm start",
    "start:backend-local": "cd ./server && npm run start-local",
    "start:frontend": "wait-on http://localhost:5001/api/calculations && cd ./client && npm start",
    "prepare": "husky",
    "spellcheck": "npx cspell --config ./cspell.json \"./**/*.jsx\" --no-progress --no-exit-code",
    "copy:env": "bash scripts/copy-env.sh",
    "compose:all": "bash scripts/copy-env.sh all && bash scripts/compose.sh all",
    "compose:app": "bash scripts/copy-env.sh app && bash scripts/compose.sh app",
    "compose:local-db": "bash scripts/copy-env.sh local-db && bash scripts/compose.sh local-db",
    "start:app-local-db": "concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run start:backend-local\" \"npm run start:frontend\" "
  },
  "dependencies": {
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@cspell/cspell-json-reporter": "^8.19.2",
    "concurrently": "^9.1.2",
    "cspell": "^8.19.2",
    "husky": "^9.1.7",
    "lerna": "^8.2.1"
  },
  "volta": {
    "node": "20.10.0"
  }
}
